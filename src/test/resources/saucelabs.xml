<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Selenium Test Suites" verbose="1" parallel="classes" thread-count="100" data-provider-thread-count="100">
    <parameter name="runOnSauceLabs" value="true"/>
    <test name="Chrome Test Suite">
        <parameter name="browserName" value="chrome"/>
        <parameter name="browserVersion" value="110"/>
        <parameter name="platformName" value="Windows 10"/>
        <parameter name="runOnSauceLabs" value="true"/>

        <packages>
            <package name="com.saucelabs"/>
        </packages>
    </test>
</suite>